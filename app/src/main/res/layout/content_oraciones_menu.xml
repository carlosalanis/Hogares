                    android:layout_height="match_parent"
                    android:layout_alignEnd="@+id/cap12_text"
                    android:layout_alignRight="@+id/cap12_text"
                    android:text="ESTUDIO: 12"
                    android:textSize="10sp" />


                <TextView
                    android:id="@+id/cap12_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/bienaventurado12_text"
